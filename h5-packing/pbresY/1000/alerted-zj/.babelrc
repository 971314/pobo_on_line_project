{
  "presets": [ "es2015", "stage-0"],

  /* if you want to use babel runtime, uncomment the following line */
   "plugins": [
          "transform-vue-jsx",
          ["transform-runtime", {
              "polyfill": true,
              "regenerator": true
          }]
      ],
  "env": {
    "build": {
      "optional": ["optimisation", "minification"]
    }
  }
}
